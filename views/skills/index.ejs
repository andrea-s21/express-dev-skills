<%- include('../partials/header') %>

<h1>Skills</h1>
<ul>
  <% skills.forEach(function(s) { %>
    <li>
      <form action="/skills/<%= s.id %>?_method=DELETE"
        class="delete-form" method="POST">
        <button type="submit">X</button>
      </form>
      <a href="/skills/<%= s.id %>">
        <%= s.skill %>
      </a>
        - 
      <%= s.learned ? 'learned' : 'not learned' %>
    </li>
  <% }); %>
</ul>
<%- include('../partials/footer') %>